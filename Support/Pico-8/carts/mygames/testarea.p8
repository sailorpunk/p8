pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

function _init()
 player={
  x=63,
  y=63,
  f=false,
  sp=16
 }
end

function _update()
	if btn(⬅️) then
	 player.x-=1
	 player.f=true
 end
 if btn(➡️) then
  player.x+=1
  player.f=false
 end
 if btn(⬆️) then
  player.y-=1
 end
 if btn(⬇️) then
  player.y+=1
 end

end

function _draw()
	cls(1)
	spr(player.sp,player.x,player.y,2,1,player.f)
 print(player.x)
 print(player.y)
end


